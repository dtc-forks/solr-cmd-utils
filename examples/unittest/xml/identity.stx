<?xml version="1.0"?>
<stx:transform xmlns:stx="http://stx.sourceforge.net/2002/ns"
               version="1.0">
    <stx:template match="@*|node()" visibility="global">
        <!-- pass the documents through -->
        <stx:copy attributes="@*|node()">
            <stx:process-children />
        </stx:copy>
    </stx:template>
</stx:transform>